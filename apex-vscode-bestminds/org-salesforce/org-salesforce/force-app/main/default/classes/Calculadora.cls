public class Calculadora {
    public Calculadora() {

    }

    public Integer Soma (Integer a, Integer b) {
        return a + b;
    }

    public Integer Subtracao (Integer a, Integer b) {      
        return a - b;
    }

    public Integer Divisao  (Integer a, Integer b) {
        return a / b;
    }

    public Integer Multiplicacao (Integer a, Integer b) {
        return a * b;
    }

}